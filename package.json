{
    "name": "mittelgrau-starterkit",
    "version": "2.0.0",
    "main": ".static/src/index.js",
    "license": "MIT",
    "scripts": {
        "dev": "parcel watch 'static/src/index.js' --out-dir static/ --public-url ../",
        "serve": "eleventy --serve --quiet",
        "build": "eleventy build && parcel build './static/src/index.js' --out-dir 'static' --public-url . --no-cache && eleventy build"
    },
    "devDependencies": {
        "@11ty/eleventy": "^0.11.1",
        "@11ty/eleventy-img": "^0.6.0",
        "@tailwindcss/postcss7-compat": "^2.0.2",
        "autoprefixer": "^9.8.6",
        "cssnano": "^4.1.10",
        "eleventy-plugin-error-overlay": "^0.1.2",
        "eleventy-plugin-typeset": "^1.0.0",
        "html-minifier": "^4.0.0",
        "parcel-bundler": "^1.3.1",
        "parcel-plugin-notifier": "^1.0.0",
        "postcss": "^7.0.35",
        "postcss-easing-gradients": "^3.0.1",
        "postcss-nested-import": "^0.1.0",
        "postcss-preset-env": "^6.7.0",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2"
    }
}
