{
    "name": "mittelgrau-starterkit",
    "version": "1.2.0",
    "main": "./src/index.js",
    "license": "MIT",
    "scripts": {
        "dev": "parcel watch ./src/index.js --out-dir ./static --public-url .",
        "serve": "eleventy --serve --quiet",
